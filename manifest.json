{
    "manifest_version": 3,
    "name": "Great Suspender URLs Fixer",
    "description": "Fix broken Great Suspender URLs currently opened",
    "version": "0.0.1",
    "icons": {
        "16": "/icons/icon_16.png",
        "48": "/icons/icon_48.png",
        "128": "/icons/icon_128.png"
    },
    "action": {
        "default_title": "Fix URLs"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}